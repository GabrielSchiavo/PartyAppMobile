{
  "expo": {
    "name": "Party App Mobile",
    "slug": "party-app-mobile",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./src/assets/icon2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gabrielschiavo.partyappmobile"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
