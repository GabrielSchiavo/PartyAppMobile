{
  "expo": {
    "name": "Party App Mobile",
    "slug": "party-app-mobile",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./src/assets/icons/icon2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/icons/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/adaptive-icon2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The application needs access to your photos to allow you to use them in registering events."
        }
      ]
    ]
  }
}
